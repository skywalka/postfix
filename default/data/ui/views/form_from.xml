<form>
  <label>Form Search - From</label>

  <searchTemplate>process=postfix* from=$from$ | table _time host queue_id from size</searchTemplate>

  <fieldset>
      <input type="text" token="from">
	<label>Enter the from address:</label>
      </input>
      <input type="time" />
  </fieldset>

  <row>
      <table>
        <title>Table</title>
        <option name="showPager">true</option>
        <option name="count">20</option>
      </table>
  </row>

  <row>
      <event>
            <title>Results</title>
            <option name="count">50</option>
      </event>
  </row>

</form>

