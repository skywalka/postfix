<form>
  <label>Form Search - To</label>

  <searchTemplate>process=postfix* to=$to$ | table _time host relay queue_id to status reason delay</searchTemplate>

  <fieldset>
      <input type="text" token="to">
	<label>Enter the to address:</label>
      </input>
      <input type="time" />
  </fieldset>

  <row>
      <table>
        <title>Table</title>
        <option name="showPager">true</option>
        <option name="count">20</option>
      </table>
  </row>

  <row>
      <event>
            <title>Results</title>
            <option name="count">50</option>
      </event>
  </row>

</form>

